{
  "name": "smart_html",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install-client": "npm install",
    "install-server": "pip install -r requirements.txt",
    "install-all": "npm run install-client && npm run install-server",
    "run-client": "vite --config smart_html_client/vite.config.js",
    "run-server": "python smart_html_server/main.py",
    "dev": "concurrently --names \"client,server\" --prefix-colors \"blue,magenta\" \"npm run run-client\" \"npm run run-server\""
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "sass": "^1.69.5",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-svgr": "^4.2.0"
  }
}
